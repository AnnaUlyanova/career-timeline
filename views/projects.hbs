<div class="page-header">
        <h1 id='projects-header'>Projects</h1>
        <p>all projects done as part of the learning journey at EDA</p>
</div>



  {{#each projects}}
    <div class="row projects">
        <div class="col-md-4">
          <a href="{{project_url}}" target="_blank"><img src="{{image_url}}" class="projects-img"></a>
        </div>
        <div class="col-md-6">
          <h3>{{name}} <a href="{{git}}"><img src="/img/git-black.png" class="projects-git"/></a></h3>
          <p class="projects-descr">{{description}}</p>
          <p class="projects-tech">Technologies used: {{tech}}</p>
        </div>
    </div>
  {{/each}}
